/* QCheckBox */

QCheckBox::indicator,
QGroupBox::indicator {
  width: 12px;
  height: 12px;
  border: 1px solid #D6D6D6;
  border-radius: 3px;
  margin: 4px 4px 4px 0;
}
QCheckBox::indicator:unchecked,
QGroupBox::indicator:unchecked {
  border: 1px solid #D6D6D6;
  background: #4B4B4B;
  image: none;
}
QCheckBox::indicator:checked,
QGroupBox::indicator:checked {
  border: 1px solid #D6D6D6;
  background: #D6D6D6;
  image: url(:/img/checked.svg);
}
QCheckBox::indicator:pressed,
QGroupBox::indicator:pressed {
  border: 1px solid #F1F1F1;
  background: #F1F1F1;
  image: url(:/img/checked.svg);
}

/* QComboBox */

QComboBox {
  background: #848484;
  border: none;
  border-radius: 4px;
  padding: 2px 10px;
}
QComboBox::item {
  color: #000000;
  background: #F0F0F0;
}
QComboBox::item::selected {
  color: #FFFFFF;
  background: #3F9BFA;
}
QComboBox::drop-down {
  width: 16px;
  background: #D6D6D6;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
QComboBox::drop-down::pressed {
  background: #F1F1F1;
}
QComboBox::down-arrow {
  image: url(:/img/down_arrow.svg);
}

/* QLineEdit */

QLineEdit {
  background: #4B4B4B;
  border: 1px solid #CACACA;
  padding: 2px;
}
QLineEdit:focus {
  border: 1px solid #FFFFFF;
}

/* QPushButton */

QPushButton {
  background: #848484;
  border: none;
  border-radius: 4px;
  color: whitesmoke;
  min-width: 76px;
  min-height: 15px;
  padding: 3px 6px;
}
QPushButton:focus {
  border: 1px solid #D6D6D6;
  outline: none;
}
QPushButton:pressed {
  background: #676767;
}
QPushButton:disabled {
  background: transparent;
  border: 1px solid #6E6E6E;
  color: #6F6F6F;
}</string>
