   <string notr="true">QWidget {
  font: 12px &quot;Noto Sans&quot;, &quot;FreeSans&quot;;
  background: #3B3B3B;
  color: whitesmoke;
}

/* QHeaderView for QTableWidget has to be set here */

QTableWidget QHeaderView::section {
  padding: 4px 2px;
  background: #6E6E6E;
  border: none;
}

/* QRadioButton */

QRadioButton::indicator {
  width: 8px;
  height: 8px;
  border-radius: 7px;
  padding: 2px;
  margin: 4px 4px 4px 0;
}
QRadioButton::indicator:unchecked {
  border: 1px solid #D6D6D6;
  image: none;
}
QRadioButton::indicator:checked {
  border: 1px solid #D6D6D6;
  background: #D6D6D6;
  image: url(:/img/selected.svg);
}
QRadioButton::indicator:pressed {
  border: 1px solid #F1F1F1;
  background: #F1F1F1;
  image: url(:/img/selected.svg);
}

/* QCheckBox */

QCheckBox::indicator,
QGroupBox::indicator,
QListView::indicator {
  width: 12px;
  height: 12px;
  border: 1px solid #D6D6D6;
  border-radius: 3px;
  margin: 4px 4px 4px 0;
}
QCheckBox::indicator:unchecked,
QGroupBox::indicator:unchecked,
QListView::indicator:unchecked {
  border: 1px solid #D6D6D6;
  background: #3B3B3B;
  image: none;
}
QCheckBox::indicator:checked,
QGroupBox::indicator:checked,
QListView::indicator:checked {
  border: 1px solid #D6D6D6;
  background: #D6D6D6;
  image: url(:/img/checked.svg);
}
QCheckBox::indicator:pressed,
QGroupBox::indicator:pressed,
QListView::indicator:pressed {
  border: 1px solid #F1F1F1;
  background: #F1F1F1;
  image: url(:/img/checked.svg);
}

/* QComboBox */

QComboBox {
  background: #848484;
  border: none;
  border-radius: 4px;
  padding: 2px 10px;
}
QComboBox::item {
  color: #000000;
  background: #F0F0F0;
}
QComboBox::item::selected {
  color: #FFFFFF;
  background: #3F9BFA;
}
QComboBox::drop-down {
  width: 16px;
  background: #D6D6D6;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
QComboBox::drop-down::pressed {
  background: #F1F1F1;
}
QComboBox::down-arrow {
  image: url(:/img/down_arrow.svg);
}

/* QLineEdit */

QLineEdit {
  background: #3B3B3B;
  border: 1px solid #CACACA;
  padding: 2px;
}
QLineEdit:focus {
  border: 1px solid #FFFFFF;
}

/* QPushButton */

QPushButton {
  background: #848484;
  border: none;
  border-radius: 4px;
  color: whitesmoke;
  min-width: 76px;
  min-height: 15px;
  padding: 3px 6px;
}
QPushButton:focus {
  border: 1px solid #D6D6D6;
  outline: none;
}
QPushButton:pressed {
  background: #676767;
}
QPushButton:disabled {
  background: transparent;
  border: 1px solid #6E6E6E;
  color: #6F6F6F;
}

/* QSpinBox */

QSpinBox {
  background: #6E6E6E;
  border: none;
  border-radius: 4px;
  padding: 2px 10px;
}
QSpinBox::disabled {
  color: #6F6F6F;
}
QSpinBox::up-button {
  background: #D6D6D6;
  border: none;
  border-top-right-radius: 4px;
  width: 16px;
}
QSpinBox::down-button {
  background: #D6D6D6;
  border: none;
  border-bottom-right-radius: 4px;
  width: 16px;
}
QSpinBox::up-button::pressed,
QSpinBox::down-button::pressed {
  background: #F1F1F1;
}
QSpinBox::up-arrow {
  background: url(:/img/up_arrow.svg);
}
QSpinBox::down-arrow {
  background: url(:/img/down_arrow.svg);
}

/* QGroupBox */

QGroupBox {
  border-top: 1px solid #6E6E6E;
  margin-top: 0.5em;
  padding-top: 15px;
}
QGroupBox::title {
  subcontrol-origin: margin;
  padding: 0 6px 0 2px;
  left: 0px;
}

QFrame[frameShape=&quot;4&quot;] {
  border: none;
  max-height: 1px;
  background: #A0A0A0;
}
QFrame[frameShape=&quot;5&quot;] {
  border: none;
  max-width: 1px;
  background: #A0A0A0;
}

/* QMenuBar and stuff */

QMenuBar {
  background: #272727;
}
QMenuBar::item {
  color: #FFFFFF;
  background: transparent;
}
QMenuBar::item:selected {
	background: #0770DD;
}

QMenu {
  background: #272727;
}
QMenu::item {
  color: #FFFFFF;
  background: transparent;
}
QMenu::item:selected {
	background: #0770DD;
}
QMenu::item:checked {
}
QMenu::item::disabled {
	color: #696969;
}
QMenu::separator {
	background: #4A4A4A;
	height: 2px;
	margin: 2px 0;
}

/* buttons */

#muteButton,
#previousButton,
#playButton,
#nextButton,
#sidebarButton,
#repeatButton,
#shuffleButton,
#addButton,
#clearButton {
  border: none;
  min-width: -1;
  min-height: -1;
}

/* Playlist group */

#playlistSearchBox,
#librarySearchBox {
  color: #A5A5A5;
	background-color: #515151;
	border: none;
	font-size: 13px;
	padding: 5px 3px;
}
#playlistSearchBox:focus,
#librarySearchBox:focus {
	border-color: #008098;
}

#playlistWidget,
#libraryWidget {
  margin-top: 10px;
}

/* time labels */
#durationLabel {
	font-size: 10px;
}
#seperatorLabel,
#remainingLabel {
	color: gray;
	font-size: 10px;
}
#indexLabel {
  font-size: 11px;
}

/* playlistWidget */

QListView {
	outline: none;
}
#playlistWidget::item {
	background: #202020;
	padding: 4px 2px;
}
#playlistWidget::item:selected {
	background: #515151;
}
#playlistWidget:item:selected:active,
#playlistWidget:item:selected:!active {
	color: whitesmoke;
}

#repeatButton,
#shuffleButton,
#addButton,
#clearButton {
  padding: 2px;
  margin: 1px;
  border: none;
}

#openFileButton,
#openUrlButton,
#viewLibraryButton {
  padding: 5px 10px 5px 10px;
  color: #E5E5E5;
  text-align: left;
  border: none;
  padding-left: 38px;
}

/* QScrollBar */

QScrollBar:vertical {
	width: 4px;
}
QScrollBar::handle:vertical {
	background: #767676;
	border-radius: 2px;
	min-height: 10px;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
	border: none;
}
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical  {
	background: #515151;
	margin: 0 2px;
}
QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
	background: transparent;
}

QScrollBar:horizontal {
	height: 4px;
}
QScrollBar::handle:horizontal {
	background: #767676;
	border-radius: 2px;
	min-width: 10px;
}
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
	border: none;
}
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
	background: #515151;
	margin: 2px 0;
}
QScrollBar::left-arrow:horizontal,
QScrollBar::right-arrow:horizontal {
	background: transparent;
}
QToolTip {
  background-color: #202020;
  color: #A5A5A5;
}</string>
